{
    "name": "onedesign/craft-shopify",
    "description": "Bring Shopify products into Craft",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft shopify"
    ],
    "support": {
        "docs": "https://github.com/onedesign/craft-shopify/blob/master/README.md",
        "issues": "https://github.com/onedesign/craft-shopify/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "One Design Company",
            "homepage": "https://onedesigncompany.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.2.0",
        "phpclassic/php-shopify": "^1.1"
    },
    "require-dev": {
      "craftcms/cms": "^3.2.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "onedesign\\craftshopify\\": "src/"
        }
    },
    "extra": {
        "name": "Craft Shopify",
        "handle": "craft-shopify",
        "developer": "One Design Company",
        "developerUrl": "https://onedesigncompany.com/",
        "documentationUrl": "https://github.com/onedesign/craft-shopify/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/onedesign/craft-shopify/master/CHANGELOG.md",
        "components": {
            "craftShopifyService": "onedesign\\craftshopify\\services\\CraftShopifyService"
        },
        "class": "onedesign\\craftshopify\\CraftShopify"
    }
}
